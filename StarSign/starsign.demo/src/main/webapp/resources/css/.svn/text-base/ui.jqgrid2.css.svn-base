/*Grid*/
.ui-jqgrid2 {position: relative; font-size:12px;}
.ui-jqgrid2 .ui-jqgrid-view {position: relative;left:0px; top: 0px; padding: .0em;}
/* caption*/
.ui-jqgrid2 .ui-jqgrid-titlebar {padding: .3em .2em .2em .3em; position: relative; border-left: 0px none;border-right: 0px none; border-top: 0px none;}
.ui-jqgrid2 .ui-jqgrid-title { float: left; margin: .1em 0 .2em; }
.ui-jqgrid2 .ui-jqgrid-titlebar-close { position: absolute;top: 50%; width: 19px; margin: -10px 0 0 0; padding: 1px; height:18px;}.ui-jqgrid .ui-jqgrid-titlebar-close span { display: block; margin: 1px; }
.ui-jqgrid2 .ui-jqgrid-titlebar-close:hover { padding: 0; }
/* header*/
.ui-jqgrid2 .ui-jqgrid-hdiv {position: relative; margin: 0em;padding: 0em; overflow-x: hidden; overflow-y: auto; border-left: 0px none !important; border-top : 0px none !important; border-right : 0px none !important;}
.ui-jqgrid2 .ui-jqgrid-hbox {float: left; padding-right: 20px;}
.ui-jqgrid2 .ui-jqgrid-htable {table-layout:fixed; margin:0em; border-collapse: separate;}
.ui-jqgrid2 .ui-jqgrid-htable th {height:22px;padding: 0 2px 0 2px;}
.ui-jqgrid2 .ui-jqgrid-htable th div {overflow: hidden; position:relative; height:17px;}
.ui-th-column, .ui-jqgrid2 .ui-jqgrid-htable th.ui-th-column {overflow: hidden;white-space: nowrap;text-align:center;border-top : 0px none;border-bottom : 0px none;}
.ui-th-ltr, .ui-jqgrid .ui-jqgrid-htable th.ui-th-ltr {border-left : 0px none;}
.ui-th-rtl, .ui-jqgrid .ui-jqgrid-htable th.ui-th-rtl {border-right : 0px none;}
.ui-jqgrid2 .ui-th-div-ie {white-space: nowrap; zoom :1; height:17px;}
.ui-jqgrid2 .ui-jqgrid-resize {height:20px !important;position: relative; cursor :e-resize;display: inline;overflow: hidden;}
.ui-jqgrid2 .ui-grid-ico-sort {overflow:hidden;position:absolute;display:inline; cursor: pointer !important;}
.ui-jqgrid2 .ui-icon-asc {margin-top:-3px; height:12px;}
.ui-jqgrid2 .ui-icon-desc {margin-top:3px;height:12px;}
.ui-jqgrid2 .ui-i-asc {margin-top:0px;height:16px;}
.ui-jqgrid2 .ui-i-desc {margin-top:0px;margin-left:13px;height:16px;}
.ui-jqgrid2 .ui-jqgrid-sortable {cursor:pointer;}
.ui-jqgrid2 tr.ui-search-toolbar th { border-top-width: 1px !important; border-top-color: inherit !important; border-top-style: ridge !important }
tr.ui-search-toolbar input {margin: 1px 0px 0px 0px}
tr.ui-search-toolbar select {margin: 1px 0px 0px 0px}
/* body */ 
.ui-jqgrid2 .ui-jqgrid-bdiv {position: relative; margin: 0em; padding:0; overflow: auto; text-align:left;}
.ui-jqgrid2 .ui-jqgrid-btable {table-layout:fixed; margin:0em; border-collapse: separate;}
.ui-jqgrid2 tr.jqgrow { outline-style: none; }
.ui-jqgrid2 tr.jqgrow td {font-weight: normal; overflow: hidden; white-space: pre; height: 22px;padding: 0 2px 0 2px;border-bottom-width: 1px; border-bottom-color: inherit; border-bottom-style: solid;}
.ui-jqgrid2 tr.jqgfirstrow td {padding: 0 2px 0 2px;border-right-width: 1px; border-right-style: solid;}
.ui-jqgrid2 tr.jqgroup td {font-weight: normal; overflow: hidden; white-space: pre; height: 22px;padding: 0 2px 0 2px;border-bottom-width: 1px; border-bottom-color: inherit; border-bottom-style: solid;}
.ui-jqgrid2 tr.jqfoot td {font-weight: bold; overflow: hidden; white-space: pre; height: 22px;padding: 0 2px 0 2px;border-bottom-width: 1px; border-bottom-color: inherit; border-bottom-style: solid;}
.ui-jqgrid2 tr.ui-row-ltr td {text-align:center;}
.ui-jqgrid2 tr.ui-row-ltr td .md {color: #47799b; margin-right: 0px; text-decoration: underline;}
.ui-jqgrid2 tr.ui-row-ltr td .dl {color: #de4444; margin-right: 0px; text-decoration: underline;}
.ui-jqgrid2 tr.ui-row-ltr td .ds {color: #a0a0a0; margin-right: 0px; text-decoration: none;}
.ui-jqgrid2 tr.ui-row-rtl td {text-align:right;}

/*
.jqGridEvenRowColor3 {background:#47799b; color:#000000;}
.jqGridEvenRowColor4 {background:#a0a0a0; color:#000000;}
.jqGridMouseOver{background: #aaaaaa; color: #333;}
.jqGridMouseClick{background: #bbbbbb;color: #333; font-weight: bold;}
*/

.ui-jqgrid2 td.jqgrid-rownum { padding: 0 2px 0 2px; margin: 0px; border: 0px none;}
.ui-jqgrid2 .ui-jqgrid-resize-mark { width:2px; left:0; background-color:#777; cursor: e-resize; cursor: col-resize; position:absolute; top:0; height:100px; overflow:hidden; display:none;	border:0 none;}
/* footer */
.ui-jqgrid2 .ui-jqgrid-sdiv {position: relative; margin: 0em;padding: 0em; overflow: hidden; border-left: 0px none !important; border-top : 0px none !important; border-right : 0px none !important;}
.ui-jqgrid2 .ui-jqgrid-ftable {table-layout:fixed; margin-bottom:0em;}
.ui-jqgrid2 tr.footrow td {font-weight: bold; overflow: hidden; white-space:nowrap; height: 21px;padding: 0 2px 0 2px;border-top-width: 1px; border-top-color: inherit; border-top-style: solid;}
.ui-jqgrid2 tr.footrow-ltr td {text-align:left;border-right-width: 1px; border-right-color: inherit; border-right-style: solid;}
.ui-jqgrid2 tr.footrow-rtl td {text-align:right;border-left-width: 1px; border-left-color: inherit; border-left-style: solid;}
/* Pager*/
.ui-jqgrid2 .ui-jqgrid-pager { border-left: 0px none !important;border-right: 0px none !important; border-bottom: 0px none !important; margin: 0px !important; padding: 0px !important; position: relative; height: 25px;white-space: nowrap;overflow: hidden;}
.ui-jqgrid2 .ui-pager-control {position: relative;}
.ui-jqgrid2 .ui-pg-table {position: relative; border-collapse: separate; padding-bottom:2px; width:auto; margin: 0em;}
.ui-jqgrid2 .ui-pg-table td {font-weight:normal; vertical-align:middle; padding:1px;}
.ui-jqgrid2 .ui-pg-button  { height:19px !important;}
.ui-jqgrid2 .ui-pg-button span { display: block; margin: 1px; float:left;}
.ui-jqgrid2 .ui-pg-button:hover { padding: 0px; }
.ui-jqgrid2 .ui-state-disabled:hover {padding:1px;}
.ui-jqgrid2 .ui-pg-input { height:13px;font-size:.8em; margin: 0em;color:black;}
.ui-jqgrid2 .ui-pg-selbox {font-size:.8em; line-height:18px; display:block; height:18px; margin: 0em;}
.ui-jqgrid2 .ui-separator {height: 18px; border-left: 1px solid #ccc ; border-right: 1px solid #ccc ; margin: 1px; float: right;}
.ui-jqgrid2 .ui-paging-info {font-weight: normal;height:19px; margin-top:3px;margin-right:4px;}
.ui-jqgrid2 .ui-jqgrid-pager .ui-pg-div {padding:1px 0;float:left;list-style-image:none;list-style-position:outside;list-style-type:none;position:relative;}
.ui-jqgrid2 .ui-jqgrid-pager .ui-pg-button { cursor:pointer; }
.ui-jqgrid2 .ui-jqgrid-pager .ui-pg-div  span.ui-icon {float:left;margin:0 2px;}
.ui-jqgrid2 td input, .ui-jqgrid td select .ui-jqgrid td textarea { margin: 0em;}
.ui-jqgrid2 td textarea {width:auto;height:auto;}
.ui-jqgrid2 .ui-jqgrid-toppager {border-left: 0px none !important;border-right: 0px none !important; border-top: 0px none !important; margin: 0px !important; padding: 0px !important; position: relative; height: 25px !important;white-space: nowrap;overflow: hidden;}
/*subgrid*/
.ui-jqgrid2 .ui-jqgrid-btable .ui-sgcollapsed span {display: block;}
.ui-jqgrid2 .ui-subgrid {margin:0em;padding:0em; width:100%;}
.ui-jqgrid2 .ui-subgrid table {table-layout: fixed; border-collapse: separate;}
.ui-jqgrid2 .ui-subgrid tr.ui-subtblcell td {height:18px;border-right-width: 1px; border-right-color: inherit; border-right-style: solid;border-bottom-width: 1px; border-bottom-color: inherit; border-bottom-style: solid;}
.ui-jqgrid2 .ui-subgrid td.subgrid-data {border-top:  0px none !important;}
.ui-jqgrid2 .ui-subgrid td.subgrid-cell {border-width: 0px 0px 1px 0px;}
.ui-jqgrid2 .ui-th-subgrid {height:20px;}
/* loading */
.ui-jqgrid2 .loading {position: absolute; top: 45%;left: 45%;width: auto;z-index:101;padding: 6px; margin: 5px;text-align: center;font-weight: bold;display: none;border-width: 2px !important;}
.ui-jqgrid2 .jqgrid-overlay {display:none;z-index:100;}
* html .jqgrid-overlay {width: expression(this.parentNode.offsetWidth+'px');height: expression(this.parentNode.offsetHeight+'px');}
* .jqgrid-overlay iframe {position:absolute;top:0;left:0;z-index:-1;width: expression(this.parentNode.offsetWidth+'px');height: expression(this.parentNode.offsetHeight+'px');}
/* end loading div */
/* toolbar */
.ui-jqgrid2 .ui-userdata {border-left: 0px none;	border-right: 0px none;	height : 21px;overflow: hidden;	}
/*Modal Window */
.ui-jqdialog { display: none; width: 300px; position: absolute; padding: .2em; font-size:12px; overflow:visible;}
.ui-jqdialog .ui-jqdialog-titlebar { padding: .3em .2em; position: relative;  }
.ui-jqdialog .ui-jqdialog-title { margin: .1em 0 .2em; } 
.ui-jqdialog .ui-jqdialog-titlebar-close { position: absolute;  top: 50%; width: 19px; margin: -10px 0 0 0; padding: 1px; height: 18px; }

.ui-jqdialog .ui-jqdialog-titlebar-close span { display: block; margin: 1px; }
.ui-jqdialog .ui-jqdialog-titlebar-close:hover, .ui-jqdialog .ui-jqdialog-titlebar-close:focus { padding: 0; }
.ui-jqdialog-content, .ui-jqdialog .ui-jqdialog-content { border: 0; padding: .3em .2em; background: none; height:auto;}
.ui-jqdialog .ui-jqconfirm {padding: .4em 1em; border-width:3px;position:absolute;bottom:10px;right:10px;overflow:visible;display:none;height:80px;width:220px;text-align:center;}
/* end Modal window*/
/* Form edit */
.ui-jqdialog-content .FormGrid {margin: 0px;}
.ui-jqdialog-content .EditTable { width: 100%; margin-bottom:0em;}
.ui-jqdialog-content .DelTable { width: 100%; margin-bottom:0em;}
.EditTable td input, .EditTable td select, .EditTable td textarea {margin: 0em;}
.EditTable td textarea { width:auto; height:auto;}
.ui-jqdialog-content td.EditButton {text-align: right;border-top: 0px none;border-left: 0px none;border-right: 0px none; padding-bottom:5px; padding-top:5px;}
.ui-jqdialog-content td.navButton {text-align: center; border-left: 0px none;border-top: 0px none;border-right: 0px none; padding-bottom:5px; padding-top:5px;}
.ui-jqdialog-content input.FormElement {vertical-align:middle; border: 1px solid #848685; background: #272727; padding: 2px 5px 0; width: 120px; height: 17px; color:#dbdbdb;}
.ui-jqdialog-content .data-line {padding-top:.1em;border: 0px none;}

.ui-jqdialog-content .CaptionTD {text-align: left; vertical-align: middle;border: 0px none; padding: 2px;white-space: nowrap;}
.ui-jqdialog-content .DataTD {padding: 2px; border: 0px none; vertical-align: top;}
.ui-jqdialog-content .form-view-data {white-space:pre}
.fm-button { display: inline-block; margin:0 4px 0 0; padding: .4em .5em; text-decoration:none !important; cursor:pointer; position: relative; text-align: center; zoom: 1; }
.fm-button-icon-left { padding-left: .8em; padding-right: .8em;}
.fm-button-icon-right { padding-left: .8em; padding-right: .8em; }
.fm-button-icon-left .ui-icon { right: auto; left: .2em; margin-left: 0; position: absolute; top: 50%; margin-top: -8px; }
.fm-button-icon-right .ui-icon { left: auto; right: .2em; margin-left: 0; position: absolute; top: 50%; margin-top: -8px;}
#nData, #pData { float: left; margin:3px;padding: 0; width: 15px; }
/* End Eorm edit */
/*.ui-jqgrid .edit-cell {}*/
.ui-jqgrid2 .selected-row, div.ui-jqgrid2 .selected-row td {font-style : normal;border-left: 0px none;}
/* Tree Grid */
.ui-jqgrid2 .tree-wrap {float: left; position: relative;height: 18px;white-space: nowrap;overflow: hidden;}
.ui-jqgrid2 .tree-minus {position: absolute; height: 18px; width: 18px; overflow: hidden;}
.ui-jqgrid2 .tree-plus {position: absolute;	height: 18px; width: 18px;	overflow: hidden;}
.ui-jqgrid2 .tree-leaf {position: absolute;	height: 18px; width: 18px;overflow: hidden;}
.ui-jqgrid2 .treeclick {cursor: pointer;}
/* moda dialog */
.jqmOverlay { background-color: #000; }
* iframe.jqm {position:absolute;top:0;left:0;z-index:-1;width: expression(this.parentNode.offsetWidth+'px');height: expression(this.parentNode.offsetHeight+'px');}
.ui-jqgrid-dnd tr td {border-right-width: 1px; border-right-color: inherit; border-right-style: solid; height:20px}
/* RTL Support */
.ui-jqgrid2 .ui-jqgrid-title-rtl {float:right;margin: .1em 0 .2em; }
.ui-jqgrid2 .ui-jqgrid-hbox-rtl {float: right; padding-left: 20px;}
.ui-jqgrid2 .ui-jqgrid-resize-ltr {float: right;margin: -2px -2px -2px 0px;}
.ui-jqgrid2 .ui-jqgrid-resize-rtl {float: left;margin: -2px 0px -1px -3px;}
.ui-jqgrid2 .ui-sort-rtl {left:0px;}
.ui-jqgrid2 .tree-wrap-ltr {float: left;}
.ui-jqgrid2 .tree-wrap-rtl {float: right;}
.ui-jqgrid2 .ui-ellipsis {text-overflow:ellipsis; -moz-binding:url('ellipsis-xbl.xml#ellipsis');}
.ui-searchFilter { display: none; position: absolute; z-index: 770; overflow: visible;}
.ui-searchFilter table {position:relative; margin:0em; width:auto}
.ui-searchFilter table td {margin: 0em; padding: 1px;}
.ui-searchFilter table td input, .ui-searchFilter table td select {margin: 0.1em;}
.ui-searchFilter .ui-state-default { cursor: pointer; }
.ui-searchFilter .divider hr {margin: 1px; }